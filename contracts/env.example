# Sepolia Network Configuration
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
PRIVATE_KEY=your_private_key_here

# Sepolia FHEVM Addresses
# These are the official Sepolia testnet addresses from Zama documentation
FHEVM_REGISTRY=0x000000000000000000000000000000000000005a
FHEVM_VERIFIER=0x000000000000000000000000000000000000005b
FHEVM_GATEWAY=0x000000000000000000000000000000000000005c

# Contract Deployment Configuration
CONTRACT_OWNER=your_contract_owner_address_here
INITIAL_SUPPLY=1000000000000000000000000

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas Reporting Configuration
# Set REPORT_GAS=true to enable gas reporting in tests
REPORT_GAS=false
# Optional: CoinMarketCap API key for USD gas cost calculation
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
